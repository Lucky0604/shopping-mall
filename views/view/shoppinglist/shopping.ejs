<body>
    <div class="page-group">
        <div class="page page-current" id="shopping">
            <header class="bar bar-nav">
                <h1 class="title">Shopping Car</h1>
            </header>
            <div class="content">
                <div class="content-block">
                    <ul>
                        <li><a href="#shopping-item">shopping item</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="page" id="shopping-item">
            <header class="bar bar-nav">
                <a href="#shopping" class="button button-link button-nav pull-left back">
                    <span class="icon icon-left">Back</span>
                </a>
                <h1 class="title">Item details</h1>
            </header>
            <div class="content">
                <div class="content-block">
                    Click <a href="#" class="back">Back</a> return last page.
                </div>
            </div>
        </div>
    </div>
    <!-- bottom bar -->
    <script type='text/javascript' src='//g.alicdn.com/sj/lib/zepto/zepto.min.js' charset='utf-8'></script>
    <script type='text/javascript' src='//g.alicdn.com/msui/sm/0.5.9/js/sm.min.js' charset='utf-8'></script>
